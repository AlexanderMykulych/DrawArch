{
  "version": "0.1.0",
  "name": "coya-ts-cli",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "bin": {
    "coya-ts": "dist/index.js"
  },
  "scripts": {
    "start": "tsdx watch",
    "dev": "pnpm build && cd dist && node index.js -p ttest",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "author": "AlexMykulych",
  "module": "dist/.esm.js",
  "size-limit": [
    {
      "path": "dist/.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.5",
    "@types/figlet": "^1.5.4",
    "@types/node": "^18.6.1",
    "husky": "^7.0.4",
    "size-limit": "^7.0.5",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "clear": "^0.1.0",
    "commander": "^8.3.0",
    "figlet": "^1.5.2",
    "picocolors": "^1.0.0",
    "@vitejs/plugin-vue": "^3.0.1",
    "@vue/compiler-sfc": "^3.2.31",
    "unplugin-auto-import": "^0.11.0",
    "unplugin-vue-components": "^0.22.0",
    "vite": "~3.0.9",
    "vue": "^3.2.37",
    "@intlify/vite-plugin-vue-i18n": "^6.0.0",
    "@vue/test-utils": "2.0.2",
    "@vueuse/core": "^9.1.0",
    "@vueuse/head": "^0.7.9",
    "cypress": "^10.4.0",
    "eslint": "^8.21.0",
    "https-localhost": "^4.7.1",
    "pinia": "2.0.21",
    "pnpm": "^7.8.0",
    "unocss": "^0.45.5",
    "vite-plugin-inspect": "^0.6.0",
    "vite-plugin-pages": "0.25.0",
    "vite-plugin-pwa": "^0.12.3",
    "vite-plugin-vue-layouts": "^0.7.0",
    "vite-ssg": "^0.20.2",
    "vitest": "^0.21.0",
    "vue-demi": "^0.13.6",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.3",
    "vue-tsc": "^0.39.5"
  }
}
