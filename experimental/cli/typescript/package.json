    "@vue/compiler-sfc": "^3.2.31",
    "unplugin-auto-import": "^0.15.0",
    "unplugin-vue-components": "^0.24.0",
    "vue": "^3.2.47",
    "@intlify/vite-plugin-vue-i18n": "^6.0.0",
    "@vue/test-utils": "^2.3.0",
    "@vueuse/head": "^0.7.9",
    "cypress": "^10.4.0",
    "eslint": "^8.35.0",
    "https-localhost": "^4.7.1",
    "pinia": "2.0.21",
    "pnpm": "^7.28.0",
    "unocss": "^0.50.3",
