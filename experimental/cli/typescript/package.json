{
  "version": "0.1.0",
  "name": "coya-ts-cli",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "bin": {
    "coya-ts": "dist/index.js"
  },
  "scripts": {
    "start": "tsdx watch",
    "dev": "pnpm build && cd dist && node index.js -p ttest",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "author": "AlexMykulych",
  "module": "dist/.esm.js",
  "size-limit": [
    {
      "path": "dist/.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.5",
    "@types/figlet": "^1.5.4",
    "@types/node": "^18.14.6",
    "husky": "^7.0.4",
    "size-limit": "^7.0.5",
    "tsdx": "^0.14.1",
    "tslib": "~2.4.0",
    "typescript": "^4.9.5",
    "@antfu/eslint-config": "^0.36.0",
    "@unocss/reset": "^0.50.3",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/test-utils": "^2.3.0",
    "@vueuse/core": "^9.13.0",
    "eslint": "^8.35.0",
    "jsdom": "^21.1.0",
    "pnpm": "^7.28.0",
    "taze": "^0.9.0",
    "unocss": "^0.50.3",
    "unplugin-auto-import": "^0.15.0",
    "unplugin-vue-components": "^0.24.0",
    "vite": "^4.1.4",
    "vite-plugin-pages": "^0.28.0",
    "vitest": "^0.29.2",
    "vue": "^3.2.47",
    "vue-termui": "*",
    "vue-tsc": "^1.2.0",
    "neo4j-driver": "~5.5.0",
    "ws": "~8.12.0",
    "@iconify-json/carbon": "^1.1.16",
    "vue-router": "^4.1.6"
  },
  "dependencies": {
    "clear": "^0.1.0",
