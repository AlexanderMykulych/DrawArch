{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "vitest",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "coya-ts-analizer",
  "author": "AlexMykulych",
  "module": "dist/typescript-analizer.esm.js",
  "size-limit": [
    {
      "path": "dist/typescript-analizer.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/typescript-analizer.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.5",
    "husky": "^7.0.4",
    "size-limit": "^7.0.5",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5",
    "vite": "^2.8.0",
    "vitest": "^0.3.2"
  },
  "dependencies": {
    "@antv/g6": "^4.5.1",
    "@antv/hierarchy": "^0.6.8",
    "@antv/layout": "^0.1.24",
    "@types/node": "^17.0.17",
    "fs": "^0.0.1-security"
  }
}
