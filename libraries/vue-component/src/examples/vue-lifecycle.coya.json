				"to": "initInject"
			}
		},
		{
			"newBlock": [
				{
					"beforeCreate": "beforeCreate"
				},
				{
					"compile": {
						"label": "new palette block"
					}
				},
				{
					"compileEl": {
						"label": "new palette block"
					}
				},
				{
					"createApp": {
						"label": "new palette block"
					}
				},
				{
					"mounted": {
						"label": "new palette block"
					}
				},
				{
					"unmounted": {
						"label": "new palette block"
					}
				},
				{
					"virtualRerender": {
						"label": "new palette block"
					}
				}
			],
			"connect": [
