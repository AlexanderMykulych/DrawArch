					"w": "has_template_opt.w",
					"h": "has_template_opt.h"
				},
				"css": {
					"fill": "#3e6b94"
				},
				"label": "Compile template into render function"
			},
			"compileEl": {
				"position": {
					"x": "has_template_opt.x + has_template_opt.w + 50",
					"y": "compile.y",
					"w": "compile.w",
					"h": "compile.h"
				},
				"css": {
					"fill": "#3e6b94"
				},
				"label": "Compile el's innerHTML as template"
			},
			"createApp": {
				"position": {
					"x": "has_template_opt.x",
					"y": "compile.y + compile.h + 100",
					"w": "has_template_opt.w",
					"h": "has_template_opt.h"
				},
				"css": {
					"fill": "#00FF9D",
					"color": "#121212"
				},
				"label": "Create app.$el and append it to el"
			},
			"mounted": {
				"position": {
					"x": "createApp.x + createApp.w/4",
					"y": "createApp.y + createApp.h + 100",
					"w": "createApp.w/2",
					"h": "createApp.h"
				},
				"css": {
					"fill": "#00B0FF"
				},
				"label": "Mounted"
			},
			"unmounted": {
				"position": {
					"x": "mounted.x",
					"y": "mounted.y + mounted.h + 200",
					"w": "mounted.w",
					"h": "mounted.h"
				},
				"css": {
					"fill": "#3e6b94"
				},
				"label": "Unmounted"
			},
			"virtualRerender": {
				"position": {
					"x": "mounted.x + mounted.w + 300",
					"y": "mounted.y",
					"w": "mounted.w*2",
					"h": "mounted.h"
				},
				"css": {
					"fill": "#3e6b94"
				},
				"label": "Virtual DOM re-rendered and patch"
			},
			"line_0_1": {
				"css": {
					"stroke": "#2900FF"
				}
			}
		}
	}
}