{
	"name": "vue-lifecycle-json",
	"blocks": {
		"start": {
			"label": "app = Vue.createApp(options); app.mount(el)"
		}
	},
	"phases": [
		{
			"newBlock": {
                "init": "init events & lifecycle",
                "has_template_opt": {
                    "label": "new palette block"
                }
			},
			"connect": {
				"from": "start",
				"to": "init"
			}
		},
		{
			"newBlock": {
				"initInject": "Init injections & reactivity"
			},
			"connect": {
				"from": "init",
				"to": "initInject"
			}
		},
		{
            "newBlock": [
                {
                    "beforeCreate": "beforeCreate"
                },
                {
                    "block_new_new": {
                        "label": "new palette block"
                    }
                }
            ],
            "connect": [
                {
                    "from": "init",
                    "to": "beforeCreate",
                    "name": "line_init_beforeCreate"
                },
                {
                    "from": "initInject",
                    "to": "has_template_opt"
                },
                {
                    "from": "has_template_opt",
                    "to": "block_new_new"
                }
            ]
		}
	],
	"style": {
		"blocks": {
			"_": {
				"css": {
					"color": "white",
					"fill": "#3ab881",
					"fontSize": "0.1em"
				}
            },
			"init": {
				"position": {
					"x": "start.x",
					"y": "start.y + start.h + 20",
					"w": "start.w",
					"h": "start.h"
				}
			},
			"initInject": {
				"position": {
					"x": "init.x",
                    "y": "init.y + start.h + 20",
					"w": "init.w",
					"h": "init.h"
				}
			},
			"beforeCreate": {
				"position": {
					"x": "init.x - init.w - 40",
					"y": "init.y + init.h",
					"w": "init.w",
					"h": "initInject.y - init.y - init.h"
				},
				"css": {
					"fill": "none",
					"color": "red",
					"stroke": "red"
				}
			},
			"line_init_beforeCreate": {
				"position": {
                    "x1": "line_1_1.x1",
                    "y1": "line_1_1.y1 + (line_1_1.y2 - line_1_1.y1) / 2",
                    "y2": "line_1_1.y1 + (line_1_1.y2 - line_1_1.y1) / 2"
				},
				"css": {
					"fill": "red",
					"stroke": "red",
					"stroke-dasharray": 3
				}
            },
            "block_new_new": {
                "position": {
                    "x": "has_template_opt.x",
                    "y": "has_template_opt.y + has_template_opt.h + 20",
                    "w": "has_template_opt.w",
                    "h": "has_template_opt.h"
                },
                "css": {
                    "fill": "#3e6b94"
                }
            },
            "start": {
                "position": {
                    "x": "133.5509796142578",
                    "y": "16.78569793701172",
                    "w": "232.4490203857422",
                    "h": "98.13617706298828"
                },
                "css": {
                    "fill": "#3e6b94"
                }
            },
            "has_template_opt": {
                "position": {
                    "x": "initInject.x",
                    "y": "initInject.y + initInject.h + 20",
                    "w": "initInject.w",
                    "h": "start.h"
                },
                "css": {
                    "fill": "#3e6b94"
                },
                "label": "Has template options"
			}
		}
	}
}