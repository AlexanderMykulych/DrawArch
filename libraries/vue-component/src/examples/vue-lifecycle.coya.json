				},
				{
					"from": "mounted",
					"to": "unmounted"
				}
			]
		}
	],
	"style": {
		"blocks": {
			"_": {
				"css": {
					"color": "white",
					"fill": "#FF89CF",
					"fontSize": "1.1em",
					"rx": "25",
					"stroke": "none"
				}
			},
			"init": {
				"position": {
					"x": "start.x",
					"y": "start.y + start.h + 80",
					"w": "start.w",
					"h": "start.h"
				}
			},
			"initInject": {
				"position": {
