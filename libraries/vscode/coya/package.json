{
    "name": "coya",
    "displayName": "coya",
    "description": "",
    "version": "0.9.20",
    "publisher": "AlexMykulych",
    "engines": {
        "vscode": "^1.60.0"
    },
    "repository": "https://github.com/AlexanderMykulych/coya",
    "npm": {
        "packageManager": "pnpm"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:coya.helloWorld",
        "onCommand:coya.open"
    ],
    "main": "./out/coya.cjs.js",
    "contributes": {
        "commands": [
            {
                "command": "coya.open",
                "title": "Run coya"
            }
        ],
        "menus": {
            "editor/title": [
                {
                    "when": "resourceExtname == .ts && resourceFilename =~ /\\.coya\\./",
                    "command": "coya.open",
                    "group": "navigation"
                },
                {
                    "when": "resourceExtname == .json && resourceFilename =~ /\\.coya\\./",
                    "command": "coya.open",
                    "group": "navigation"
                }
            ],
            "explorer/context": [
                {
                    "when": "(resourceExtname == .ts || resourceExtname == .json) && resourceFilename =~ \\.coya\\.",
                    "command": "coya.open",
                    "group": "navigation"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "build": "npm run compile",
        "build2": "tsup src/extension.ts --dts --external=vscode",
        "compile": "vite build",
        "watch": "tsc -watch -p ./",
        "pretest": "npm compile && npm lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.1.3",
        "@types/mocha": "^8.2.2",
        "@types/vscode": "^1.60.0",
        "@typescript-eslint/eslint-plugin": "^5.10.2",
        "@typescript-eslint/parser": "^4.29.3",
        "eslint": "^8.35.0",
        "glob": "^7.1.7",
        "mocha": "^8.4.0",
        "vscode-test": "^1.5.2"
    },
    "dependencies": {
        "@rollup/plugin-commonjs": "^21.0.1",
        "@rollup/plugin-node-resolve": "^13.1.2",
        "@types/node": "^18.14.6",
        "@types/ws": "~8.5.3",
        "@vue-reactivity/watch": "^0.1.6",
        "@vue/reactivity": "^3.2.38",
        "child_process": "^1.0.2",
        "coya-core": "~0.9.20",
        "coya-util": "~0.9.20",
        "os": "^0.1.2",
        "rollup-plugin-polyfill-node": "^0.8.0",
        "tsup": "^5.11.10",
        "tsutils": "^3.21.0",
        "typescript": "^4.9.5",
        "vite": "^4.1.4",
        "vue": "^3.2.47",
        "ws": "~8.12.0",
        "@vitejs/plugin-vue": "^4.0.0",
        "@vue/compiler-sfc": "^3.2.31",
        "unplugin-auto-import": "^0.15.0",
        "unplugin-vue-components": "^0.24.0",
        "@intlify/vite-plugin-vue-i18n": "^6.0.0",
        "@vue/test-utils": "^2.3.0",
        "@vueuse/core": "^9.13.0",
        "@vueuse/head": "^0.7.9",
        "cypress": "^10.4.0",
        "eslint": "^8.35.0",
        "https-localhost": "^4.7.1",
        "pinia": "2.0.21",
        "pnpm": "^7.28.0",
        "unocss": "^0.50.3",
        "vite-plugin-inspect": "^0.6.0",
        "vite-plugin-pages": "^0.28.0",
        "vite-plugin-pwa": "^0.12.3",
        "vite-plugin-vue-layouts": "^0.7.0",
        "vite-ssg": "^0.20.2",
        "vitest": "^0.29.2",
        "vue-demi": "^0.13.6",
        "vue-i18n": "^9.2.2",
        "vue-router": "^4.1.6",
        "vue-tsc": "^1.2.0",
        "@antfu/eslint-config": "^0.36.0",
        "@unocss/reset": "^0.50.3",
        "jsdom": "^21.1.0",
        "taze": "^0.9.0",
        "vue-termui": "*",
        "neo4j-driver": "~5.5.0",
        "@iconify-json/carbon": "^1.1.16"
    }
}
