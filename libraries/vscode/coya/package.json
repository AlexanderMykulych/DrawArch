{
    "name": "coya",
    "displayName": "coya",
    "description": "",
    "version": "0.9.14",
    "publisher": "AlexMykulych",
    "engines": {
        "vscode": "^1.60.0"
    },
    "repository": "https://github.com/AlexanderMykulych/coya",
    "npm": {
        "packageManager": "pnpm"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:coya.helloWorld",
        "onCommand:coya.open"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "coya.open",
                "title": "Run coya"
            }
        ],
        "menus": {
            "editor/title": [
                {
                    "when": "resourceExtname == .ts && resourceFilename =~ /\\.coya\\./",
                    "command": "coya.open",
                    "group": "navigation"
                },
                {
                    "when": "resourceExtname == .json && resourceFilename =~ /\\.coya\\./",
                    "command": "coya.open",
                    "group": "navigation"
                }
            ],
            "explorer/context": [
                {
                    "when": "(resourceExtname == .ts || resourceExtname == .json) && resourceFilename =~ \\.coya\\.",
                    "command": "coya.open",
                    "group": "navigation"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "build": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm compile && npm lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.1.3",
        "@types/mocha": "^8.2.2",
        "@types/vscode": "^1.60.0",
        "@typescript-eslint/eslint-plugin": "^4.29.3",
        "@typescript-eslint/parser": "^4.29.3",
        "eslint": "^7.32.0",
        "glob": "^7.1.7",
        "mocha": "^8.4.0",
        "vscode-test": "^1.5.2"
    },
    "dependencies": {
        "@types/ws": "^8.2.0",
        "@vue-reactivity/watch": "^0.1.6",
        "@vue/reactivity": "^3.2.12",
        "child_process": "^1.0.2",
        "coya-core": "~0.9.14",
        "tsutils": "^3.21.0",
        "typescript": "^4.4.2",
        "ws": "^8.2.3"
    }
}
