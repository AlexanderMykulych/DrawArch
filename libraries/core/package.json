{
    "version": "0.9.18",
    "license": "MIT",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "files": [
        "dist",
        "src"
    ],
    "engines": {
        "node": ">=10"
    },
    "scripts": {
        "start": "tsdx watch",
        "build2": "pnpm tsc && pnpm tsdx:build",
        "build": "vite build",
        "tsdx:watch": "tsdx watch --transpileOnly",
        "tsdx:build": "tsdx build --transpileOnly",
        "tsc": "tsc --emitDeclarationOnly --noEmit false --outDir dist --declaration",
        "tsc:watch": "yarn tsc --watch",
        "buildi": "tsc --build",
        "build:watch": "tsdx build",
        "test": "vitest",
        "lint": "tsdx lint",
        "prepare": "tsdx build --transpileOnly",
        "version2": "tsdx --version",
        "size": "size-limit",
        "analyze": "size-limit --why"
    },
    "husky": {
        "hooks": {
            "pre-commit": "tsdx lint"
        }
    },
    "prettier": {
        "printWidth": 80,
        "semi": true,
        "singleQuote": true,
        "trailingComma": "es5"
    },
    "name": "coya-core",
    "author": "alex.mykulych",
    "module": "dist/coya-core.es.js",
    "size-limit": [
        {
            "path": "dist/core-core.cjs.production.min.js",
            "limit": "10 KB"
        },
        {
            "path": "dist/core-core.esm.js",
            "limit": "10 KB"
        }
    ],
    "pnpm": {
        "overrides": {
            "typescript": "^4.4.2",
            "@typescript-eslint/eslint-plugin": "^4.29.3",
            "@typescript-eslint/parser": "^4.29.3"
        }
    },
    "resolutions": {
        "**/typescript": "^4.4.3",
        "**/@typescript-eslint/eslint-plugin": "^4.29.3",
        "**/@typescript-eslint/parser": "^4.29.3"
    },
    "devDependencies": {
        "@size-limit/preset-small-lib": "^7.0.4",
        "@types/jest": "^27.0.1",
        "@typescript-eslint/eslint-plugin": "^4.29.3",
        "@typescript-eslint/parser": "^4.29.3",
        "eslint": "^8.3.0",
        "husky": "^7.0.4",
        "tsdx": "^0.14.1",
        "tslib": "^2.3.1",
        "typescript": "^4.4.2",
        "vitest": "^0.0.112"
    },
    "dependencies": {
        "@antv/hierarchy": "^0.6.8",
        "@antv/layout": "^0.1.24",
        "@types/lodash": "^4.14.172",
        "@types/node": "^16.7.1",
        "@vueuse/core": "^7.4.1",
        "coya-util": "~0.1.0",
        "csstype": "^3.0.8",
        "curved-arrows": "^0.1.0",
        "jest": "^27.0.6",
        "lodash": "^4.17.21",
        "rollup-plugin-commonjs": "^10.1.0",
        "stylis": "^4.0.10",
        "vite": "^2.7.6",
        "vue": "^3.2.26"
    }
}
