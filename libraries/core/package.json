{
    "version": "0.9.8",
    "license": "MIT",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "files": [
        "dist",
        "src"
    ],
    "engines": {
        "node": ">=10"
    },
    "scripts": {
        "start": "tsdx watch",
        "build": "pnpm tsc && pnpm tsdx:build",
        "tsdx:watch": "tsdx watch --transpileOnly",
        "tsdx:build": "tsdx build --transpileOnly",
        "tsc": "tsc --emitDeclarationOnly --noEmit false --outDir dist --declaration",
        "tsc:watch": "yarn tsc --watch",
        "buildi": "tsc --build",
        "build:watch": "tsdx build",
        "test": "tsdx test",
        "lint": "tsdx lint",
        "prepare": "tsdx build --transpileOnly",
        "version2": "tsdx --version",
        "size": "size-limit",
        "analyze": "size-limit --why"
    },
    "husky": {
        "hooks": {
            "pre-commit": "tsdx lint"
        }
    },
    "prettier": {
        "printWidth": 80,
        "semi": true,
        "singleQuote": true,
        "trailingComma": "es5"
    },
    "name": "coya-core",
    "author": "alex.mykulych",
    "module": "dist/coya-core.esm.js",
    "size-limit": [
        {
            "path": "dist/core-core.cjs.production.min.js",
            "limit": "10 KB"
        },
        {
            "path": "dist/core-core.esm.js",
            "limit": "10 KB"
        }
    ],
    "pnpm": {
        "overrides": {
            "typescript": "^4.4.2",
            "@typescript-eslint/eslint-plugin": "^4.29.3",
            "@typescript-eslint/parser": "^4.29.3"
        }
    },
    "resolutions": {
        "**/typescript": "^4.4.2",
        "**/@typescript-eslint/eslint-plugin": "^4.29.3",
        "**/@typescript-eslint/parser": "^4.29.3"
    },
    "devDependencies": {
        "@size-limit/preset-small-lib": "^5.0.3",
        "@types/jest": "^27.0.1",
        "@typescript-eslint/eslint-plugin": "^4.29.3",
        "@typescript-eslint/parser": "^4.29.3",
        "eslint": "^7.32.0",
        "husky": "^7.0.1",
        "size-limit": "^5.0.3",
        "tsdx": "^0.14.1",
        "tslib": "^2.3.1",
        "typescript": "^4.4.2"
    },
    "dependencies": {
        "@types/lodash": "^4.14.172",
        "@types/node": "^16.7.1",
        "csstype": "^3.0.8",
        "jest": "^27.0.6",
        "lodash": "^4.17.21",
        "stylis": "^4.0.10",
        "vue": "3.2.4"
    }
}
