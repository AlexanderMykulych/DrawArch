{
    "name": "coya-core",
    "version": "0.9.20",
    "license": "MIT",
    "author": "alex.mykulych",
    "files": [
        "dist",
        "src"
    ],
    "main": "dist/coya-core.cjs.js",
    "typings": "src/index.ts",
    "engines": {
        "node": ">=10"
    },
    "module": "dist/coya-core.es.js",
    "scripts": {
        "start": "tsdx watch",
        "build2": "pnpm tsc && pnpm tsdx:build",
        "build": "vite build",
        "tsdx:watch": "tsdx watch --transpileOnly",
        "tsdx:build": "tsdx build --transpileOnly",
        "tsc": "tsc --emitDeclarationOnly --noEmit false --outDir dist --declaration",
        "tsc:watch": "yarn tsc --watch",
        "buildi": "tsc --build",
        "build:watch": "tsdx build",
        "test": "vitest",
        "lint": "tsdx lint",
        "prepare": "tsdx build --transpileOnly",
        "version2": "tsdx --version",
        "size": "size-limit",
        "analyze": "size-limit --why"
    },
    "husky": {
        "hooks": {
            "pre-commit": "tsdx lint"
        }
    },
    "prettier": {
        "printWidth": 80,
        "semi": true,
        "singleQuote": true,
        "trailingComma": "es5"
    },
    "size-limit": [
        {
            "path": "dist/core-core.cjs.production.min.js",
            "limit": "10 KB"
        },
        {
            "path": "dist/core-core.esm.js",
            "limit": "10 KB"
        }
    ],
    "dependencies": {
        "@antv/hierarchy": "^0.6.8",
        "@antv/layout": "^0.1.24",
        "@types/lodash": "^4.14.172",
        "@types/node": "^18.6.1",
        "@vueuse/core": "^9.1.0",
        "coya-arrow": "~0.9.20",
        "coya-util": "~0.9.20",
        "csstype": "^3.0.8",
        "curved-arrows": "^0.1.0",
        "jest": "^27.0.6",
        "lodash": "^4.17.21",
        "rollup-plugin-commonjs": "^10.1.0",
        "stylis": "^4.0.10",
        "vite": "~3.0.9",
        "vue": "^3.2.37",
        "@vitejs/plugin-vue": "^3.0.1",
        "@vue/compiler-sfc": "^3.2.31",
        "unplugin-auto-import": "^0.11.0",
        "unplugin-vue-components": "^0.22.0",
        "@intlify/vite-plugin-vue-i18n": "^6.0.0",
        "@vue/test-utils": "2.0.2",
        "@vueuse/head": "^0.7.9",
        "cypress": "^10.4.0",
        "eslint": "^8.21.0",
        "https-localhost": "^4.7.1",
        "pinia": "2.0.21",
        "pnpm": "^7.8.0",
        "unocss": "^0.45.5",
        "vite-plugin-inspect": "^0.6.0",
        "vite-plugin-pages": "0.25.0",
        "vite-plugin-pwa": "^0.12.3",
        "vite-plugin-vue-layouts": "^0.7.0",
        "vite-ssg": "^0.20.2",
        "vitest": "^0.21.0",
        "vue-demi": "^0.13.6",
        "vue-i18n": "^9.2.2",
        "vue-router": "^4.1.3",
        "vue-tsc": "^0.39.5"
    },
    "devDependencies": {
        "@size-limit/preset-small-lib": "^7.0.5",
        "@types/jest": "^27.0.1",
        "@typescript-eslint/eslint-plugin": "^5.10.2",
        "@typescript-eslint/parser": "^4.29.3",
        "eslint": "^8.21.0",
        "husky": "^7.0.4",
        "tsdx": "^0.14.1",
        "tslib": "^2.3.1",
        "typescript": "^4.7.4",
        "vite-plugin-dts": "~1.2.1",
        "vitest": "^0.21.0"
    }
}
