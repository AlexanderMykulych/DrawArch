{
    "version": "0.1.0",
    "name": "coya-gif",
    "license": "MIT",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "files": [
        "dist",
        "src"
    ],
    "engines": {
        "node": ">=10"
    },
    "scripts": {
        "start": "tsdx watch",
        "build": "tsdx build",
        "test": "tsdx test",
        "lint": "tsdx lint",
        "prepare": "tsdx build",
        "size": "size-limit",
        "analyze": "size-limit --why"
    },
    "husky": {
        "hooks": {
            "pre-commit": "tsdx lint"
        }
    },
    "prettier": {
        "printWidth": 80,
        "semi": true,
        "singleQuote": true,
        "trailingComma": "es5"
    },
    "author": "AlexMykulych",
    "module": "dist/gif.esm.js",
    "size-limit": [
        {
            "path": "dist/gif.cjs.production.min.js",
            "limit": "10 KB"
        },
        {
            "path": "dist/gif.esm.js",
            "limit": "10 KB"
        }
    ],
    "devDependencies": {
        "@size-limit/preset-small-lib": "^5.0.3",
        "husky": "^7.0.2",
        "size-limit": "^5.0.3",
        "tsdx": "^0.14.1",
        "tslib": "^2.3.1",
        "typescript": "^4.4.3"
    },
    "dependencies": {
        "@types/gifencoder": "^2.0.1",
        "@types/pngjs": "^6.0.1",
        "gifencoder": "^2.0.1",
        "png-js": "^1.0.0",
        "pngjs": "^6.0.0",
        "puppeteer": "^10.2.0"
    }
}